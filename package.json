{
  "name": "app-gestor-solicitudes",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:local": "vite --mode local",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "preview": "vite preview",
    "start": "npm run build && vite preview --port 3000 --host 0.0.0.0",
    "start:prod": "vite preview --port 3000 --host 0.0.0.0",
    "serve": "vite preview --port 3000 --host 0.0.0.0",
    "docker:dev": "bash docker-scripts.sh dev",
    "docker:prod": "bash docker-scripts.sh prod",
    "docker:build": "bash docker-scripts.sh build-all",
    "docker:stop": "bash docker-scripts.sh stop",
    "docker:logs": "bash docker-scripts.sh logs",
    "docker:clean": "bash docker-scripts.sh clean",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "bash script/clean-build-pack.sh",
    "clean:deps": "bash script/clean-build-pack.sh --clean-deps",
    "lockfile:linux-win": "docker run --rm -v %cd%:/app -w /app node:20-alpine sh -c \"npm install\"",
    "lockfile:linux-bash": "docker run --rm -v $PWD:/app -w /app node:20-alpine sh -c 'npm install'"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.11.0",
    "@consalud/core": "file:consalud-core-1.0.0.tgz",
    "axios": "~1.9.0",
    "bulma": "^1.0.3",
    "cors": "~2.8.5",
    "date-fns": "~4.1.0",
    "express": "~5.1.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-datepicker": "~8.4.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0",
    "sweetalert2": "~11.21.0",
    "xml2js": "~0.6.2"
  },
  "devDependencies": {
    "@testing-library/dom": "~10.4.0",
    "@testing-library/jest-dom": "~6.6.3",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~14.6.1",
    "@types/jest": "~29.5.14",
    "@types/node": "~24.0.0",
    "@types/react": "~19.1.0",
    "@types/react-dom": "~19.1.1",
    "@vitejs/plugin-react": "~4.5.2",
    "@vitest/coverage-v8": "~1.4.0",
    "jsdom": "~24.0.0",
    "terser": "~5.40.0",
    "typescript": "~5.8.3",
    "vite": "~6.3.5",
    "vitest": "~1.4.0"
  },
  "peerDependencies": {
    "@azure/msal-browser": "^4.11.0",
    "bulma": "^1.0.3",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0"
  },
  "resolutions": {
    "jsdom": "~24.0.0"
  }
}
